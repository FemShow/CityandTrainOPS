{
  "$schema": "https://purl.org/csv-cubed/qube-config/v1",
  "title": "Peak crowding on a typical autumn weekday by city and train operator, annual from 2011",
  "description": "Peak crowding on a typical autumn weekday by city and train operator, annual data from 2011",
  "creator": "https://www.gov.uk/government/organisations/department-for-transport",
  "publisher": "https://www.gov.uk/government/organisations/department-for-transport",
  "license": "http://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/",
  "keywords": [
    "trains",
    "overcrowding",
    "standing",
    "services",
    "arrivals"
  ],
  "templates": {
    "observation-status": {
      "type": "attribute",
      "label": "Observation Status",
      "description": "Indicates the observation status.",
      "uri": "http://purl.org/linked-data/sdmx/2009/attribute#obsStatus"
    }
  },
  "columns": {
    "Year": {
      "from_template": "government-year"
    },
    "City": false,
    "ONS Code": {
      "from_template": "statistical-geography"
    },
    "Train operator": false,
    "AM peak arrivals (07:00-09:59): Number of services": {
      "type": "observations",
      "measure": {
        "label": "Number of services",
        "from_existing": "https://purl.org/csv-cubed/resources/count"
      },
      "data_type": "integer",
      "unit": {
        "label": "integer",
        "from_existing": "http://qudt.org/vocab/unit/INT"
      }
    },
    "PM peak arrivals (16:00-18:59): Number of services": {
      "type": "observations",
      "measure": {
        "label": "Number of services",
        "from_existing": "https://purl.org/csv-cubed/resources/count"
      },
      "data_type": "integer",
      "unit": {
        "label": "integer",
        "from_existing": "http://qudt.org/vocab/unit/INT"
      }
    },
    "AM peak arrivals (07:00-09:59): PIXC": {
      "type": "observations"
    },
    "Notes": {
      "from_template": "observation-status",
      "describes_observations": "AM peak arrivals (07:00-09:59): PIXC"
    },
    "AM peak arrivals (07:00-09:59): Passengers standing": {
      "type": "observations"
    },
    "Notes1": {
      "from_template": "observation-status",
      "describes_observations": "AM peak arrivals (07:00-09:59): Passengers standing"
    },
    "PM peak arrivals (16:00-18:59): PIXC": {
      "type": "observations"
    },
    "Notes2": {
      "from_template": "observation-status",
      "describes_observations": "PM peak arrivals (16:00-18:59): PIXC",
      "label": "[x]",
      "description": "This information not provided."
    },
    "Notes3": {
      "from_template": "observation-status",
      "describes_observations": "PM peak arrivals (16:00-18:59): PIXC",
      "label": "[r]",
      "description": "Due to the confidentiality of the passenger counts for individual services, figures are not shown in instances where they would be based on fewer than three services."
    },
    "PM peak arrivals (16:00-18:59): Passengers standing": {
      "type": "observations"
    },
    "Notes4": {
      "from_template": "observation-status",
      "describes_observations": "PM peak arrivals (16:00-18:59): Passengers standing",
      "label": "[x]",
      "description": "This information not provided."
    },
    "Notes5": {
      "from_template": "observation-status",
      "describes_observations": "PM peak arrivals (16:00-18:59): Passengers standing",
      "label": "[r]",
      "description": "Due to the confidentiality of the passenger counts for individual services, figures are not shown in instances where they would be based on fewer than three services."
    }
  }
}
