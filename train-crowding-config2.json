{
  "$schema": "https://purl.org/csv-cubed/qube-config/v1",
  "title": "Peak crowding on a typical autumn weekday by city and train operator, annual from 2011",
  "description": "Peak crowding on a typical autumn weekday by city and train operator, annual data from 2011",
  "creator": "https://www.gov.uk/government/organisations/department-for-transport",
  "publisher": "https://www.gov.uk/government/organisations/department-for-transport",
  "license": "http://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/",
  "keywords": [
    "trains",
    "overcrowding",
    "standing",
    "services",
    "arrivals"
  ],
  "templates": {
    "observation-status": {
      "type": "attribute",
      "description": "Indicates the observation status.",
      "uri": "http://purl.org/linked-data/sdmx/2009/attribute#obsStatus",
      "label": "Observation Status",
      "required": false
    }
  },
  "columns": {
    "Year": {
      "from_template": "government-year"
    },
    "City": {
      "type": "dimension"
    },
    "ONS Code": {
      "from_template": "statistical-geography"
    },
    "Train operator": {
      "type": "dimension"
    },
    "AM peak arrivals (07:00-09:59): Number of services": {
      "type": "observations",
      "measure": {
        "label": "Count of Services arrving during AM Peak",
        "description": "AM peak arrivals is defined as 07:00-09:59.",
        "from_existing": "https://purl.org/csv-cubed/resources/measures#count"
      },
      "data_type": "integer",
      "unit": {
        "label": "Number",
        "from_existing": "http://qudt.org/vocab/unit/NUM"
      }
    },
    "AM peak arrivals (07:00-09:59): PIXC": {
      "type": "observations",
      "measure": {
        "label": "AM peak arrivals (07:00-09:59):PIXC",
        "description": "Percentage of passengers in excess of capacity during AM peak arrivals",
        "from_existing": "https://purl.org/csv-cubed/resources/measures#portion-of-population"
      },
      "data_type": "decimal",
      "unit": {
        "label": "percentage",
        "from_existing": "http://qudt.org/vocab/unit/PERCENT"
      }
    },
    "obsStatus": {
      "from_template": "observation-status",
      "describes_observations": "AM peak arrivals (07:00-09:59): PIXC"
    },
    "AM peak arrivals (07:00-09:59): Passengers standing": {
      "type": "observations",
      "measure": {
        "label": "AM peak arrivals (07:00-09:59): Passengers standing",
        "description": "Passengers standing during AM peak arrivals",
        "from_existing": "https://purl.org/csv-cubed/resources/measures#passengers"
      },
      "data_type": "integer",
      "unit": {
        "label": "Number",
        "from_existing": "http://qudt.org/vocab/unit/NUM"
      }
    }
  },
  "obsStatus1": {
    "from_template": "observation-status",
    "describes_observations": "AM peak arrivals (07:00-09:59): Passengers standing"
  },
  "PM peak arrivals (16:00-18:59): Number of services": {
    "type": "observations",
    "measure": {
      "label": "PM peak arrivals (16:00-18:59): Number of services",
      "description": "PM peak arrivals is defined as 16:00-18:59.",
      "from_existing":"https://purl.org/csv-cubed/resources/measures#Count"
    },
    "data_type": "integer",
    "unit": {
      "label": "Number",
      "from_existing": "http://qudt.org/vocab/unit/NUM"
    }
  },
  "obsStatus2": {
    "from_template": "observation-status",
    "describes_observations": "PM peak arrivals (16:00-18:59): Number of services"
  },
  "PM peak arrivals (16:00-18:59): PIXC": {
    "type": "observations",
    "measure": {
      "label": "PM peak arrivals (16:00-18:59): PIXC",
      "description": "Percentage of passengers in excess of capacity during PM peak arrivals",
      "from_existing":"https://purl.org/csv-cubed/resources/measures#portion-of-population"
    },
    "data_type": "decimal",
    "unit": {
      "label": "percentage",
      "from_existing": "http://qudt.org/vocab/unit/PERCENT"
    }
  },
  "obsStatus3": {
    "from_template": "observation-status",
    "describes_observations": "PM peak arrivals (16:00-18:59): PIXC"
  },
  "PM peak arrivals (16:00-18:59): Passengers standing": {
    "type": "observations",
    "measure": {
      "label": "PM peak arrivals (16:00-18:59): Passengers standing",
      "from_existing":"https://purl.org/csv-cubed/resources/measures#Count"
    },
    "data_type": "integer",
    "unit": {
      "label": "Number",
      "from_existing": "http://qudt.org/vocab/unit/NUM"
    }
  },
  "obsStatus4": {
    "from_template": "observation-status",
    "describes_observations": "PM peak arrivals (16:00-18:59): Passengers standing"
  }
}